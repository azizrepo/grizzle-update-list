pipeline {
    agent any
    stages {
        stage('Verify branch') {
            steps {
                echo '$GIT_BRANCH'
               
                }
            }
        
        
        stage('Docker Build') {
            steps {
                pwsh(script: 'docker images -a')
                pwsh(script: ***
                    docker images -a
                     docker build -t jemkis-pipeline .
                     docker images -a
                    ***)
                }
            }
        
        }
        
        
        
         stage('good bye') {
            steps {
                echo 'good bye'
            }
        }
    }
}
